{
  "name": "charlesxmorrissey.vercel-v2",
  "version": "1.0.0",
  "private": true,
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ],
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write '**/*.{css,html,js,md,mjs,ts,tsx}' --log-level error",
    "lint:ts:watch": "yarn lint:ts --watch",
    "lint:ts": "tsc --project tsconfig.json",
    "lint": "eslint . && yarn lint:ts",
    "prepare": "husky",
    "serve": "yarn build && npx --no serve@latest out",
    "start": "next start",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest watch",
    "test": "vitest"
  },
  "dependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "eslint": "^9.39.1",
    "eslint-config-next": "^15.5.7",
    "next": "15.5.7",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@next/eslint-plugin-next": "^15.5.7",
    "@swc/core": "1.15.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
