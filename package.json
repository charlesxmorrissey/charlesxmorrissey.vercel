    "jiti": "^2.6.1",
    "jsdom": "^27.1.0",
    "postcss-normalize": "^13.0.1",
    "prettier": "^3.6.2",
    "svgo": "^4.0.0",
    "ts-node": "^10.9.2",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "packageManager": "yarn@4.6.0"
}
