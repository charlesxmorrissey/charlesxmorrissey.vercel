{
  "name": "charlesxmorrissey",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write '**/*.{css,html,js,md,ts,tsx}' --loglevel error",
    "lint:fix": "eslint --ext .js,.ts,.tsx . --fix --max-warnings 0 .",
    "lint:js": "eslint --ext .js,.ts,.tsx .",
    "lint:styles": "stylelint --config stylelint.config.js 'src/**/*.css'",
    "lint:ts:watch": "yarn lint:ts --watch",
    "lint:ts": "tsc --project tsconfig.json",
    "lint": "yarn lint:styles && yarn lint:js && yarn lint:ts",
    "prepare": "husky install",
    "start": "next start",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "dependencies": {
    "next": "^13.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
