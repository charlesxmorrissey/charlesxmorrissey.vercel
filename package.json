{
  "name": "charlesxmorrissey.vercel-v2",
  "version": "1.0.0",
  "private": true,
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ],
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write '**/*.{css,html,js,md,mjs,ts,tsx}' --log-level error",
    "lint:styles": "stylelint '**/*.css'",
    "lint:ts:watch": "yarn lint:ts --watch",
    "lint:ts": "tsc --project tsconfig.json",
    "lint": "eslint . && yarn lint:ts && yarn lint:styles",
    "prepare": "husky",
    "serve": "yarn build && npx --no serve@latest out",
    "start": "next start",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest watch",
    "test": "vitest"
  },
  "dependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vercel/speed-insights": "^1.3.1",
    "eslint": "^9.39.3",
    "eslint-config-next": "^16.1.6",
    "next": "^16.1.6",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.1",
    "@next/eslint-plugin-next": "^16.1.6",
    "@swc/core": "1.15.11",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/identity-obj-proxy": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-istanbul": "^4.0.18",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-sort-destructure-keys": "^3.0.0",
    "eslint-plugin-sort-export-all": "^2.1.0",
    "eslint-plugin-typescript-sort-keys": "^3.3.0",
    "eslint-plugin-unused-imports": "^4.4.1",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jiti": "^2.6.1",
    "jsdom": "^28.1.0",
    "postcss-normalize": "^13.0.1",
    "prettier": "^3.8.1",
    "stylelint": "^17.3.0",
    "stylelint-config-css-modules": "^4.6.0",
    "stylelint-config-standard": "^40.0.0",
    "stylelint-order": "^7.0.1",
    "svgo": "^4.0.0",
    "ts-node": "^10.9.2",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
